	month = nov,
	year = {2016},
	pages = {917--928},
	file = {Volltext:C\:\\Users\\failensc\\Zotero\\storage\\Z56DZIYA\\Wagenmakers et al. - 2016 - Registered Replication Report Strack, Martin, & S.pdf:application/pdf},
