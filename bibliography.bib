	keywords = {alcohol use disorder, Bayesian model selection, depression, ising model, spike and slab prior},
	pages = {47--82},
	file = {Full Text PDF:C\:\\Users\\failensc\\Zotero\\storage\\JB2XQ8CZ\\Marsman et al. - 2022 - Objective Bayesian Edge Screening and Structure Se.pdf:application/pdf},
}

